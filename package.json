  },
  "keywords": [
    "npm",
    "webcomponent",
    "react",
    "vue",
    "image",
    "viewer",
    "viewerjs",
    "viewer.js",
    "html",
    "css",
    "javascript",
    "front-end",
    "web"
  ],
  "author": {
    "name": "Tarrency"
  },
  "license": "MIT",
  "module": "./dist/index.es.js",
  "main": "./dist/index.cjs.js",
